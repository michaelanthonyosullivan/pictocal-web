generator client {
  provider      = "prisma-client-js"
  engineType    = "binary"
  binaryTargets = ["native", "darwin-arm64"]
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
}

model DiaryEntry {
  id        String   @id @default(uuid())
  ownerEmail String
  entryDate DateTime
  content   String
  imageUrl  String?
}
